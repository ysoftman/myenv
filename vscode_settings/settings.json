{
    "files.eol": "\n",
    "files.associations": {
        "*.sh.*": "shellscript",
        "*.py.*": "python",
        "*.yml.*": "yaml",
        "*dockerfile*": "dockerfile",
        "*httpd.conf*": "apacheconf",
        "*nginx.*": "NGINX",
        "*makefile*": "makefile"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.vscode": true
    },
    "go.buildOnSave": "package",
    "go.lintOnSave": "package",
    "go.vetOnSave": "package",
    "go.buildTags": "",
    "go.buildFlags": [],
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.coverOnSave": false,
    "go.useCodeSnippetsOnFunctionSuggest": false,
    "go.formatTool": "goimports",
    "go.lintTool": "golint",
    "go.gocodeAutoBuild": false,
    "go.toolsEnvVars": {
        "GO111MODULE": "on"
    },
    "go.goroot": "/usr/local/go",
    "go.gopath": "/Users/ysoftman/workspace/gopath",
    // "go.goroot": "c:\\go",
    // "go.gopath": "d:\\ysoftman\\workspace\\gopath",
    "window.zoomLevel": 0,
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "workbench.sideBar.location": "left",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.iconTheme": "vscode-icons",
    "git.autofetch": true,
    "git.confirmSync": false,
    "python.formatting.provider": "autopep8",
    "C_Cpp.clang_format_path": "/usr/local/bin/clang-format",
    "C_Cpp.clang_format_style": "file",
    "C_Cpp.clang_format_fallbackStyle": "Visual Studio",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "all",
    "editor.minimap.enabled": false,
    "python.disablePromptForFeatures": [
        "pylint"
    ],
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    "explorer.confirmDelete": false,
    // Number of editors shown in the Open Editors pane.
    "explorer.openEditors.visible": 0,
    // Should display the Treeview?
    "bookmarks.treeview.visible": false,
    "bookmarks.navigateThroughAllFiles": true,
    // Show or hide the Explorer.
    "docker.showExplorer": false,
    "vsicons.projectDetection.autoReload": true,
    "explorer.confirmDragAndDrop": false,
    "editor.fontSize": 14,
    "editor.fontFamily": "monaco, menlo",
}